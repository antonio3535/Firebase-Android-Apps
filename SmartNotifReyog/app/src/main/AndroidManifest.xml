<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smart.reyog">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/ic_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_icon_rounded"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".EditPentas" />
        <activity android:name=".KelolaPentas" />
        <activity android:name=".VideoAdd" />
        <activity android:name=".InfoPentas" />
        <activity android:name=".Form_Pentas" />
        <activity android:name=".Admin_Page" />
        <activity android:name=".Home_page" />
        <activity android:name=".Register_Page" />
        <activity android:name=".Login_Page" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".NotifService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>